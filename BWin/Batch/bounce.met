set /a W[]_X=28, W[]_Y=31
set /a W[]_XA=1, W[]_YA=1
set /a W[]_ACTIVE=1, W[]_CLOSE=1, W[]_SIZE=0, W[]_CLEAR=1, W[]_SCROLL=0, W[]_EXP=4
set W[]_INIT="set /a DELT[]=1, XP[]=0"
